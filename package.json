  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview --host localhost --port 3000",
    "check": "npm-check --update",
    "prepare": "husky install; [ -z $CI ] || [ $CI  = 'false' ] && npm-merge-driver install || echo 'Skipping npm-merge-driver in CI.'",
    "pretty-quick": "pretty-quick --staged",
    "lint:styles": "stylelint \"**/*.{jsx,js,ts,tsx}\"",
    "eslint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint": "npm-run-all --continue-on-error eslint types:check",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "types:check": "tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
    "types:sync": "typesync",
    "ci": "CI=false BROWSER=none START_SERVER_AND_TEST_INSECURE=1 NODE_ENV=development start-server-and-test start https-get://localhost:3000 test-headless",
    "test:unit": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run test && npm run build",
    "setup": "npm install && npm run validate"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
